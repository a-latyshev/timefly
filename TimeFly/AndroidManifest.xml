<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.timefly"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="11"
        android:targetSdkVersion="18" />

    <application
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="timefly.activity.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="timefly.activity.CalenderActivity"
            android:label="@string/title_activity_calender" >
        </activity>
        <activity
            android:name="timefly.activity.CalenderActivity"
            android:label="@string/title_activity_calender" >
        </activity>
        <activity
            android:name="timefly.activity.CalederActivity"
            android:label="@string/title_activity_caleder" >
        </activity>
    </application>

</manifest>
